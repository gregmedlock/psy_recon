library(KEGGREST)
library(tidyverse)
table <-  read.csv(file.choose())
tablen<-table[,2:2]
View(tablen)
for (val in tablen) {
n <- keggLink("pathway", val)
for (paths in n){
q <- keggList(paths)
#print (as.character(val))
#print (as.character(q))
m = data.frame(as.character(val),as.character(q))
write.table(m, file="projects/psy_recon/results/gene_essentiality/ensemble_essentialiity/kegg_orthology_draft.csv",
append=TRUE,
col.names = FALSE,
sep = ',')
}
}
print ("done")
